<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DocLabelView">
        <attr name="line_spacing" format="dimension" />
        <attr name="label_spacing" format="dimension" />
        <attr name="child_view_height" format="dimension|enum">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="child_view_width" format="dimension|enum">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="child_view_min_width" format="dimension" />
        <attr name="line_limit" format="integer" />
    </declare-styleable>
    <declare-styleable name="CropImageView">
        <attr name="guidelines">
            <enum name="on" value="2" />
            <enum name="onTouch" value="1" />
            <enum name="off" value="0" />
        </attr>
        <attr name="fixAspectRatio" format="boolean" />
        <attr name="aspectRatioX" format="integer" />
        <attr name="aspectRatioY" format="integer" />
        <attr name="imageResource" format="reference" />
    </declare-styleable>
    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="y_offset_step" format="dimension" />
        <attr name="rotation_degrees" format="float" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>
    <declare-styleable name="ShadowLayout">
        <attr name="sl_cornerRadius" format="dimension" />
        <attr name="sl_shadowRadius" format="dimension" />
        <attr name="sl_shadowColor" format="color" />
        <attr name="sl_dx" format="dimension" />
        <attr name="sl_dy" format="dimension" />
    </declare-styleable>
    <declare-styleable name="card">
        <attr name="itemMarginTop" format="dimension" />
        <attr name="bottomMarginTop" format="dimension" />
        <attr name="yOffsetStep" format="dimension" />
        <attr name="widthHeightRate" format="float" />
    </declare-styleable>
    <declare-styleable name="CardItemView">
        <attr name="show_type" format="integer" />
    </declare-styleable>
    <declare-styleable name="KiraRatingBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="KiraTabLayout">
        <attr name="kira_tl_indicator_color" format="color" />
        <attr name="kira_tl_indicator_height" format="dimension" />
        <attr name="kira_tl_indicator_width" format="dimension" />
        <attr name="kira_tl_indicator_margin_start" format="dimension" />
        <attr name="kira_tl_indicator_margin_top" format="dimension" />
        <attr name="kira_tl_indicator_margin_end" format="dimension" />
        <attr name="kira_tl_indicator_margin_bottom" format="dimension" />
        <attr name="kira_tl_indicator_corner_radius" format="dimension" />
        <attr name="kira_tl_indicator_gravity" format="enum">
            <enum name="TOP" value="48" />
            <enum name="BOTTOM" value="80" />
        </attr>
        <attr name="kira_tl_indicator_style" format="enum">
            <enum name="NORMAL" value="0" />
            <enum name="TRIANGLE" value="1" />
            <enum name="BLOCK" value="2" />
        </attr>
        <attr name="kira_tl_indicator_width_equal_title" format="boolean" />
        <attr name="kira_tl_underline_color" format="color" />
        <attr name="kira_tl_underline_height" format="dimension" />
        <attr name="kira_tl_underline_gravity" format="enum">
            <enum name="TOP" value="48" />
            <enum name="BOTTOM" value="80" />
        </attr>
        <attr name="kira_tl_divider_color" format="color" />
        <attr name="kira_tl_divider_width" format="dimension" />
        <attr name="kira_tl_divider_padding" format="dimension" />
        <attr name="kira_tl_tab_padding" format="dimension" />
        <attr name="kira_tl_tab_space_equal" format="boolean" />
        <attr name="kira_tl_tab_width" format="dimension" />
        <attr name="kira_tl_text_size" format="dimension" />
        <attr name="kira_tl_textSelectColor" format="color" />
        <attr name="kira_tl_textUnSelectColor" format="color" />
        <attr name="kira_tl_textSelectBackground" format="reference" />
        <attr name="kira_tl_textUnSelectBackground" format="reference" />
        <attr name="kira_tl_textSelectPaddingTop" format="dimension" />
        <attr name="kira_tl_textUnSelectPaddingTop" format="dimension" />
        <attr name="kira_tl_textBold" format="enum">
            <enum name="NONE" value="0" />
            <enum name="SELECT" value="1" />
            <enum name="BOTH" value="2" />
        </attr>
        <attr name="kira_tl_textAllCaps" format="boolean" />
        <attr name="kira_tl_tab_layout" format="reference" />
        <attr name="kira_tl_dot_bg_color" format="color" />
        <attr name="kira_tl_dot_text_size" format="dimension" />
        <attr name="kira_tl_dot_text_color" format="color" />
        <attr name="kira_tl_dot_width" format="dimension" />
    </declare-styleable>
    <!--头像重叠-->
    <declare-styleable name="PileLayout">
        <attr name="PileLayout_vertivalSpace" format="dimension" />
        <attr name="PileLayout_pileWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AbsRoundImageView">
        <attr name="round_borderWidth" format="dimension|reference" />
        <attr name="round_borderColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="leftTopRadius" format="dimension|reference" />
        <attr name="rightTopRadius" format="dimension|reference" />
        <attr name="rightBottomRadius" format="dimension|reference" />
        <attr name="leftBottomRadius" format="dimension|reference" />
    </declare-styleable>
</resources>